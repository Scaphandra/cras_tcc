<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.VBox?>

<ScrollPane prefHeight="517.0" prefWidth="803.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controlador.AppControlador">
   <content>
      <VBox prefHeight="515.0" prefWidth="800.0">
         <children>
            <MenuBar id="principal" stylesheets="@estilo.css" styleClass="principal">
              <menus>
                  <Menu mnemonicParsing="false" text="Pessoas">
                    <items>
                      <MenuItem fx:id="menuPessoa" mnemonicParsing="false" onAction="#clicarPessoa" text="Gerenciar" />
                    </items>
                  </Menu>
                  <Menu fx:id="familia" mnemonicParsing="false" text="Famílias">
                    <items>
                      <MenuItem fx:id="menuFamilia" mnemonicParsing="false" onAction="#clicarFamilia" text="Gerenciar" />
                    </items>
                  </Menu>
                  <Menu mnemonicParsing="false" text="Atendimentos">
                    <items>
                      <MenuItem mnemonicParsing="false" onAction="#clicarMenuAtendimento" text="Gerenciar" />
                    </items>
                  </Menu>
                  <Menu mnemonicParsing="false" text="Funcionários">
                    <items>
                      <MenuItem mnemonicParsing="false" text="Action 1" />
                    </items>
                  </Menu>
                  <Menu mnemonicParsing="false" text="Angendamentos">
                    <items>
                      <MenuItem mnemonicParsing="false" text="Action 1" />
                    </items>
                  </Menu>
                <Menu mnemonicParsing="false" text="Ajuda">
                  <items>
                    <MenuItem fx:id="menuAjuda" mnemonicParsing="false" onAction="#clicarMenuSobre" text="Sobre" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
         </children>
      </VBox>
   </content>
</ScrollPane>
